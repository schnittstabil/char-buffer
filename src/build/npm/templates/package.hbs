{
  "name": "{{{name}}}",
  "version": "{{{version}}}",
  "description": "{{{description}}}",
  "homepage": "{{{homepage}}}",
  "keywords": [
    {{#each keywords}}{{#if @index}},
    {{/if}}"{{{this}}}"{{/each}}
  ],
  "author": "{{{author}}}",
  "license": "{{{license}}}",
  "repository": {
    "type": "{{{repository.type}}}",
    "url": "{{{repository.url}}}"
  },
  "bugs": {
    "url": "{{{bugs.url}}}"
  },
  "preferGlobal": false,
  "main": "./index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/_mocha --require 'test/inject' -R spec --recursive test/js"
  },
  "engines": {
    "node": "{{{engines.node}}}"
  },
  "dependencies": {
    {{#each dependencies}}{{#if @index}},
    {{/if}}"{{@key}}": "{{{this}}}"{{/each}}
  },
  "devDependencies": {
    "expect.js": "{{{devDependencies.[expect.js]}}}",
    "mocha": "{{{devDependencies.mocha}}}"
  }
}
