{
  "name": "{{{name}}}",
  "version": "{{{version}}}",
  "description": "{{{description}}}",
  "homepage": "{{{homepage}}}",
  "keywords": [
    {{#each keywords}}{{#if @index}},
    {{/if}}"{{{this}}}"{{/each}}
  ],
  "author": "{{{author}}}",
  "license": "{{{license}}}",
  "repository": {
    "type": "{{{repository.type}}}",
    "url": "{{{repository.url}}}"
  },
  "bugs": {
    "url": "{{{bugs.url}}}"
  },
  "preferGlobal": false,
  "main": "./index.js",
  "scripts": {
    "pretest": "cd node_modules && ln -s .. char-buffer && cd ../test && ../node_modules/.bin/bower install",
    "test": "grunt test"
  },
  "engines": {
    "node": "{{{engines.node}}}"
  },
  "dependencies": {
    {{#each dependencies}}{{#if @index}},
    {{/if}}"{{@key}}": "{{{this}}}"{{/each}}
  },
  "devDependencies": {
    "coveralls": "{{{devDependencies.coveralls}}}",
    "expect.js": "{{{devDependencies.[expect.js]}}}",
    "mocha": "{{{devDependencies.mocha}}}",
    "bower": "{{{devDependencies.bower}}}",
    "grunt": "{{{devDependencies.grunt}}}",
    "grunt-contrib-connect": "{{{devDependencies.[grunt-contrib-connect]}}}",
    "grunt-contrib-watch": "{{{devDependencies.[grunt-contrib-watch]}}}",
    "grunt-mocha-istanbul": "{{{devDependencies.[grunt-mocha-istanbul]}}}",
    "grunt-saucelabs": "{{{devDependencies.[grunt-saucelabs]}}}",
    "istanbul": "{{{devDependencies.istanbul}}}"
  }
}
