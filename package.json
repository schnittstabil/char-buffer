{
  "name": "char-buffer",
  "version": "1.0.1",
  "description": "Collect CharCodes and convert them to string.",
  "homepage": "https://github.com/schnittstabil/char-buffer",
  "keywords": [
    "Char",
    "CharCode",
    "ASCII",
    "Buffer",
    "CharBuffer",
    "NodeBuffer",
    "StringArrayBuffer",
    "StringBuffer",
    "TypedArrayBuffer",
    "TypedArray",
    "Uint16Array",
    "Array",
    "String"
  ],
  "author": "Michael Mayer <michael@schnittstabil.de>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/schnittstabil/char-buffer.git"
  },
  "bugs": {
    "url": "https://github.com/schnittstabil/char-buffer/issues"
  },
  "preferGlobal": false,
  "main": "node-buffer.js",
  "files": [
    "*-buffer.js"
  ],
  "scripts": {
    "test": "xo && ava",
    "test-cover": "xo && nyc ava",
    "test-cover-html": "npm run test-cover && nyc report --reporter=html",
    "test-browser-local": "grunt default",
    "test-browser-saucelabs": "grunt saucelabs",
    "docs-clean": "del readme.md gh-pages/api gh-pages/readme.md",
    "docs-build": "grunt docs && jsduck --builtin-classes --welcome=gh-pages/readme.md --title=char-buffer --output=gh-pages/api *-buffer.js",
    "docs": "npm run docs-clean && npm run docs-build"
  },
  "engines": {
    "node": ">= 0.10"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.15.2",
    "chalk": "^1.1.3",
    "coveralls": "^2.11.11",
    "del-cli": "^0.2.0",
    "dots": "^0.1.0",
    "execa": "^0.4.0",
    "grunt": "^1.0.1",
    "grunt-compile-handlebars": "^2.0.2",
    "grunt-contrib-connect": "^1.0.2",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-saucelabs": "^9.0.0",
    "handlebars": "^4.0.5",
    "jspm": "^0.16.39",
    "nyc": "^7.1.0-candidate3",
    "xo": "^0.16.0"
  },
  "xo": {
    "ignores": [
      "**/.nyc_output/**",
      "**/coverage/**",
      "**/gh-pages/**",
      "**/jspm_packages/**",
      "config.js",
      "build.js"
    ],
    "overrides": [
      {
        "files": "test/_mocha.js",
        "envs": [
          "mocha",
          "browser"
        ],
        "globals": [
          "xpect"
        ]
      }
    ]
  },
  "jspm": {
    "main": "typed-array-buffer",
    "format": "cjs",
    "directories": {
      "files": [
        "./abstract-char-buffer.js",
        "./string-array-buffer.js",
        "./string-buffer.js",
        "./typed-array-buffer.js"
      ],
      "lib": "."
    },
    "dependencies": {},
    "devDependencies": {}
  }
}
