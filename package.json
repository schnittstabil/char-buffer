{
  "name": "char-buffer",
  "version": "1.0.1",
  "description": "Collect CharCodes and convert them to string.",
  "homepage": "https://github.com/schnittstabil/char-buffer",
  "keywords": [
    "Char",
    "CharCode",
    "ASCII",
    "Buffer",
    "CharBuffer",
    "NodeBuffer",
    "StringArrayBuffer",
    "StringBuffer",
    "TypedArrayBuffer",
    "TypedArray",
    "Uint16Array",
    "Array",
    "String"
  ],
  "preferGlobal": false,
  "engines": {
    "node": ">= 0.10"
  },
  "author": "Michael Mayer <michael@schnittstabil.de>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/schnittstabil/char-buffer.git"
  },
  "bugs": {
    "url": "https://github.com/schnittstabil/char-buffer/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.16",
    "chalk": "^1.0",
    "coveralls": "^2.0",
    "dots": "^0.1.0",
    "execa": "^0.4.0",
    "grunt": "^1.0",
    "grunt-contrib-connect": "^1.0",
    "grunt-contrib-watch": "^1.0",
    "grunt-saucelabs": "^9.0",
    "jspm": "^0.16",
    "nyc": "^8.3",
    "rimraf": "^2.5",
    "xo": "^0.16"
  },
  "xo": {
    "ignores": [
      "**/.nyc_output/**",
      "**/docs/**",
      "**/jspm_packages/**",
      "config.js",
      "build.js"
    ],
    "overrides": [
      {
        "files": "test/_mocha.js",
        "envs": [
          "mocha",
          "browser"
        ],
        "globals": [
          "exposeMochaResults",
          "xpect"
        ]
      }
    ]
  },
  "jspm": {
    "main": "typed-array-buffer",
    "format": "cjs",
    "directories": {
      "files": [
        "./abstract-char-buffer.js",
        "./string-array-buffer.js",
        "./string-buffer.js",
        "./typed-array-buffer.js"
      ],
      "lib": "."
    },
    "dependencies": {},
    "devDependencies": {}
  },
  "main": "node-buffer.js",
  "files": [
    "*-buffer.js"
  ],
  "scripts": {
    "test": "xo && ava",
    "test-cover": "xo && nyc ava",
    "test-browser-local": "grunt default",
    "test-browser-saucelabs": "grunt saucelabs",
    "docs-clean": "rimraf docs/api docs/coverage",
    "docs-api": "jsduck --builtin-classes --welcome=readme.md --title=char-buffer --output=docs/api *-buffer.js",
    "docs-coverage": "npm run test-cover && nyc report --reporter=html --report-dir=docs/coverage",
    "docs": "npm run docs-clean && npm run docs-api && npm run docs-coverage"
  }
}
